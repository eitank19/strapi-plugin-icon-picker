{
  "name": "@jonssonworkwear/strapi-plugin-icon-picker",
  "version": "1.0.0",
  "description": "Select an icon from a list of SVG assets",
  "strapi": {
    "name": "icon-picker",
    "description": "List of icons, then pick one",
    "kind": "plugin",
    "displayName": "Icon picker"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JonssonWorkwear/strapi-plugin-icon-picker.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "release"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=16.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
